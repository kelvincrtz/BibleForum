@model BibleForum.Models.Forum.ForumIndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<table class="table table-hover">
    <tbody>
        @foreach(var forum in Model.ForumListing)
        {
            <tr>
                <td>@forum.Title</td>
                <td>@forum.Description</td>
            </tr>
        }
    </tbody>
</table>
